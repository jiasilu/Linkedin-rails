<p id="notice"><%= notice %></p>

<h1>User In Companies</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Company</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Position</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_in_companies.each do |user_in_company| %>
      <tr>
        <td><%= user_in_company.user_id %></td>
        <td><%= user_in_company.company_id %></td>
        <td><%= user_in_company.start_date %></td>
        <td><%= user_in_company.end_date %></td>
        <td><%= user_in_company.position %></td>
        <td><%= user_in_company.description %></td>
        <td><%= link_to 'Show', user_in_company %></td>
        <td><%= link_to 'Edit', edit_user_in_company_path(user_in_company) %></td>
        <td><%= link_to 'Destroy', user_in_company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User In Company', new_user_in_company_path %>
